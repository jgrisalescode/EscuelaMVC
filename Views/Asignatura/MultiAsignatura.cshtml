@model List<Asignatura>

@{
    // Se usa Model. por convención
    ViewData["Title"] = "Información Asignatura";
    // Plantillas
    // Layout = "_Layout"; // Por defecto
    Layout = "Simple"; // Propia
}


<div class="container">
    <div class="row">
        <div class="col-12">
            <h1>@ViewData["Title"]</h1>      
            <h2>Desde la Vista Parcial</h2>
            <!--Si no le paso el modelo en model a partial, trabaja con el modelo 
                que tenga la vista general en este caso este Index-->
            @foreach (var asignatura in Model)
            {
                <partial name="AsignaturaSimple" model="asignatura"/>
            }
            <p>@ViewBag.Fecha</p>
        </div>
    </div>
</div>